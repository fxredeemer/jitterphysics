<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>netstandard2.0</TargetFrameworks>
    <PackageId>JitterPhysics</PackageId>
    <Copyright>Copyright (c) 2019 Thorben Linneweber, Matthew Leibowitz, Flurin Feuerstein</Copyright>
    <Version>0.2.0.0</Version>
    <Authors>Thorben Linneweber, Matthew Leibowitz, Flurin Feuerstein</Authors>
    <Company />
    <Product>Realtime physics engine for .NET</Product>
    <Description>Jitter Physics is a fast and lightweight physics engine written in C#</Description>
    <PackageLicenseFile>LICENSE.md</PackageLicenseFile>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <RepositoryUrl>https://github.com/fxredeemer/jitterphysics</RepositoryUrl>
    <PackageTags>Jitter, Physics, 3D</PackageTags>
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
  </PropertyGroup>
  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>
  <ItemGroup>
    <None Include="..\..\LICENSE.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>
</Project>